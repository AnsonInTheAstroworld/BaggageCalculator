padding-top: 10px\">\n      <el-card class=\"box-card\" shadow=\"hover\" style=\"background-color: white\">\n        <!--航班线路-->\n        <div>\n          <div class=\"card-header\">\n            <span style=\"font-size: x-large;font-weight: lighter\">航班路线</span>\n          </div>\n          <div style=\"padding-top: 20px\">\n            <span>从</span>\n            <el-select v-model=\"start\" filterable placeholder=\"Select\" style=\"padding-left: 10px;width: 300px\">\n              <el-option\n                  v-for=\"item in Locations\"\n                  :key=\"item.value\"\n                  :label=\"item.value\"\n                  :value=\"item.value\"\n              />\n            </el-select>\n            <span style=\"padding-left: 20px\">到</span>\n            <el-select v-model=\"destination\" filterable placeholder=\"Select\" style=\"padding-left: 10px;width: 300px\">\n              <el-option\n                  v-for=\"item in Locations\"\n                  :key=\"item.value\"\n                  :label=\"item.value\"\n                  :value=\"item.value\"\n              />\n            </el-select>\n          </div>\n        </div>\n\n        <!--机票类别-->\n        <div>\n          <div class=\"card-header\" style=\"padding-top: 30px\" >\n            <span style=\"font-size: x-large;font-weight: lighter\">机票类别</span>\n          </div>\n          <div style=\"padding-top: 20px\">\n            <span>客票类型</span>\n            <el-select v-model=\"ticket\" filterable placeholder=\"Select\" style=\"padding-left: 10px;width: 300px\">\n              <el-option\n                  v-for=\"item in TicketTypes\"\n                  :key=\"item.value\"\n                  :label=\"item.value\"\n                  :value=\"item.value\"\n              />\n            </el-select>\n            <span style=\"padding-left: 20px\">客舱类型</span>\n            <el-select v-model=\"cabin\" filterable placeholder=\"Select\" style=\"padding-left: 10px;width: 300px\">\n              <el-option\n                  v-for=\"item in CabinTypes\"\n                  :key=\"item.value\"\n                  :label=\"item.value\"\n                  :value=\"item.value\"\n              />\n            </el-select>\n            <span style=\"padding-left: 20px\">会员类型</span>\n            <el-select v-model=\"membership\" filterable placeholder=\"Select\" style=\"padding-left: 10px;width: 300px\">\n              <el-option\n                  v-for=\"item in MembershipTypes\"\n                  :key=\"item.value\"\n                  :label=\"item.value\"\n                  :value=\"item.value\"\n              />\n            </el-select>\n          </div>\n        </div>\n\n        <!--行李状况-->\n        <div>\n          <div class=\"card-header\" style=\"padding-top: 30px\" >\n            <span style=\"font-size: x-large;font-weight: lighter\">行李状况</span>\n            <span style=\"padding-left: 10px;padding-bottom: 30px\">\n              <el-button\n                  style=\"font-size: x-large;width: 20px;height: 20px;font-weight: bold;background-color: cornflowerblue;color: white\"\n                  @click=\"BaggageEditVisible=true\">\n                +\n              </el-button>\n            </span>\n          </div>\n          <div style=\"padding-top: 20px\">\n            <el-table :data=\"BaggageTable\" style=\"width: 77.5%\">\n              <el-table-column prop=\"length\" contenteditable=\"true\" label=\"长度\"  />\n              <el-table-column prop=\"width\" contenteditable=\"true\" label=\"宽度\"  />\n              <el-table-column prop=\"height\" contenteditable=\"true\" label=\"高度\"  />\n              <el-table-column prop=\"weight\" contenteditable=\"true\" label=\"重量\"  />\n            </el-table>\n          </div>\n        </div>\n\n        <!--行李编辑对话框-->\n        <el-dialog v-model=\"BaggageEditVisible\" title=\"行李状况\" style=\"width: 20%\">\n          <el-form :model=\"BaggageForm\">\n            <el-form-item label=\"长度\" >\n              <el-input v-model=\"BaggageForm.length\" autocomplete=\"off\" />\n            </el-form-item>\n            <el-form-item label=\"宽度\" >\n              <el-input v-model=\"BaggageForm.width\" autocomplete=\"off\" />\n            </el-form-item>\n            <el-form-item label=\"高度\" >\n              <el-input v-model=\"BaggageForm.height\" autocomplete=\"off\" />\n            </el-form-item>\n            <el-form-item label=\"重量\" >\n              <el-input v-model=\"BaggageForm.weight\" autocomplete=\"off\" />\n            </el-form-item>\n          </el-form>\n          <template #footer>\n      <span class=\"dialog-footer\">\n        <el-button @click=\"BaggageEditVisible = false\">取消</el-button>\n        <el-button type=\"primary\" @click=\"SavaBaggage\"\n        >确认</el-button\n        >\n      </span>\n          </template>\n        </el-dialog>\n\n        <!--特殊行李-->\n        <div>\n          <div class=\"card-header\" style=\"padding-top: 30px\" >\n            <span style=\"font-size: x-large;font-weight: lighter\">特殊行李</span>\n            <span style=\"padding-left: 10px;padding-bottom: 30px\">\n              <el-button\n                  style=\"font-size: x-large;width: 20px;height: 20px;font-weight: bold;background-color: cornflowerblue;color: white\"\n                  @click=\"SpecialBaggageEditVisible=true\">\n                +\n              </el-button>\n            </span>\n          </div>\n          <div style=\"padding-top: 20px\">\n            <el-table :data=\"SpecialBaggageTable\" style=\"width: 77.5%\">\n              <el-table-column prop=\"SpecialBaggage\" contenteditable=\"true\" label=\"类型\"  />\n              <el-table-column prop=\"weight\" contenteditable=\"true\" label=\"重量\"  />\n            </el-table>\n          </div>\n        </div>\n\n        <!--行李编辑对话框-->\n        <el-dialog v-model=\"SpecialBaggageEditVisible\" title=\"特殊行李\" style=\"width:30%\">\n          <el-form :model=\"SpecialBaggageForm\">\n            <el-form-item label=\"类型\" >\n              <el-select v-model=\"SpecialBaggageForm.SpecialBaggage\" filterable placeholder=\"请选择特殊行李类型\" >\n                <el-option\n                    v-for=\"item in SpecialBaggageTypes\"\n                    :key=\"item.value\"\n                    :label=\"item.value\"\n                    :value=\"item.value\"\n                />\n              </el-select>\n            </el-form-item>\n            <el-form-item label=\"重量\" >\n              <el-input v-model=\"SpecialBaggageForm.weight\" autocomplete=\"off\" style=\"width: 30%\"/>\n            </el-form-item>\n          </el-form>\n          <template #footer>\n      <span class=\"dialog-footer\">\n        <el-button @click=\"SpecialBaggageEditVisible = false\">取消</el-button>\n        <el-button type=\"primary\" @click=\"SavaSpecialBaggage\"\n        >确认</el-button\n        >\n      </span>\n          </template>\n        </el-dialog>\n\n        <div style=\"padding-top: 20px\">\n          <el-button style=\"width: 150px;height: 50px;font-size: larger;background-color: cornflowerblue;color: white\">\n            计算行李\n          </el-button>\n        </div>\n      </el-card>\n    </div>\n\n\n    <!--更多详情-->\n    <div style=\"padding-top: 20px;padding-left: 20px\">\n      <span style=\"font-size: xx-large;font-weight: lighter\">更多详情</span>\n    </div>\n    <div style=\"padding-top: 10px;padding-bottom:20px;flex-direction: column\">\n      <el-row>\n        <el-col :span=\"8\">\n          <el-card class=\"box-card\" shadow=\"hover\" style=\"background-color: white\">\n            <template #header>\n              <div class=\"card-header\">\n                <span style=\"font-size: x-large;font-weight: lighter\">随身携带行李</span>\n              </div>\n            </template>\n            <div>\n              <span>在此处查看您可以随身携带哪些行李进入乘客舱。</span>\n            </div>\n            <div class=\"bottom\" style=\"padding-top: 10px\">\n              <el-button type=\"text\" style=\"font-weight: bold;color: cornflowerblue\">查看随身行李</el-button>\n            </div>\n          </el-card>\n        </el-col>\n        <el-col :span=\"8\" style=\"padding-left: 10px\">\n          <el-card class=\"box-card\" shadow=\"hover\" style=\"background-color: white\">\n            <template #header>\n              <div class=\"card-header\">\n                <span style=\"font-size: x-large;font-weight: lighter\">行李托运限制</span>\n              </div>\n            </template>\n            <div>\n              <span>您可在此查看我们对于托运行李的相关规定，并提前做好准备。</span>\n            </div>\n            <div class=\"bottom\" style=\"padding-top: 10px\">\n              <el-button type=\"text\" style=\"font-weight: bold;color: cornflowerblue\">行李托运限制</el-button>\n            </div>\n          </el-card>\n        </el-col>\n        <el-col :span=\"8\" style=\"padding-left: 10px\">\n          <el-card class=\"box-card\" shadow=\"hover\" style=\"background-color: white\">\n            <template #header>\n              <div class=\"card-header\">\n                <span style=\"font-size: x-large;font-weight: lighter\">特殊行李</span>\n              </div>\n            </template>\n            <div>\n              <span>在此处查看有关携带特殊行李旅行时需要注意的事项，并了解需要支付哪些费用。</span>\n            </div>\n            <div class=\"bottom\" style=\"padding-top: 10px\">\n              <el-button type=\"text\" style=\"font-weight: bold;color: cornflowerblue\">转到随身行李</el-button>\n            </div>\n          </el-card>\n        </el-col>\n      </el-row>\n\n\n    </div>\n\n  </div>\n</template>\n\n<script>\n\nexport default {\n  name: 'HomeView',\n  components: {},\n  data(){\n    return{\n      start:'',\n      destination:'',\n      Locations:[\n        {value:'中国境内（除港澳台）'},\n        {value:'夏威夷'},\n        {value:'美洲'},\n        {value:'加勒比地区'},\n        {value:'夏威夷'},\n        {value:'欧洲'},\n        {value:'非洲'},\n        {value:'中东'},\n        {value:'亚洲（除日本、巴基斯坦、新加坡、哈萨克斯坦）'},\n        {value:'日本'},\n        {value:'巴基斯坦'},\n        {value:'新加坡'},\n        {value:'哈萨克斯坦'},\n        {value:'西南太平洋'},\n        {value:'其他'}\n      ],\n      ticket:'',\n      TicketTypes:[\n        {value:'成人/儿童客票'},\n        {value:'婴儿客票'}\n      ],\n      cabin:'',\n      CabinTypes:[\n        {value:'头等舱'},\n        {value:'公务舱'},\n        {value:'悦享经济舱、超级经济舱'},\n        {value:'经济舱'}\n      ],\n      membership:'',\n      MembershipTypes:[\n        {value:'凤凰知音终身白金卡、白金卡'},\n        {value:'凤凰知音金卡、银卡'},\n        {value:'星空联盟金卡'}\n      ],\n      BaggageEditVisible:false,\n      BaggageForm:{},\n      BaggageTable:[],\n      SpecialBaggageEditVisible:false,\n      SpecialBaggageTable:[],\n      SpecialBaggageForm:{},\n      SpecialBaggageTypes:[\n        {value:'电动轮椅'}\n      ]\n    }\n  },\n  methods:{\n    SavaBaggage(){\n      this.BaggageEditVisible=false\n    },\n    SavaSpecialBaggage(){\n      this.SpecialBaggageEditVisible = false\n    }\n  }\n}\n</script>\n\n\n"]},"metadata":{},"sourceType":"module"}